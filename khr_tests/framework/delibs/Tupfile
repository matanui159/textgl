include_rules

CC_FLAGS += $(DE_INC)

: foreach debase/*.c |> !CC |> bin/debase/%B.o {OBJ}
: foreach decpp/*.cpp |> !CC |> bin/decpp/%B.o {OBJ}
: foreach deimage/*.c |> !CC |> bin/deimage/%B.o {OBJ}
: foreach depool/*.c |> !CC |> bin/depool/%B.o {OBJ}
: foreach destream/*.c |> !CC |> bin/destream/%B.o {OBJ}
: foreach dethread/*.c |> !CC |> bin/dethread/%B.o {OBJ}
: foreach deutil/*.c |> !CC |> bin/deutil/%B.o {OBJ}
: {OBJ} |> !AR |> libde.a